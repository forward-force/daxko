{
    "name": "forward-force/daxko-api-sdk",
    "description": "PHP Wrapper around Daxko API",
    "type": "library",
    "keywords": [
        "forwardforce",
        "daxko",
        "api"
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpmd/phpmd": "dev-master",
        "vimeo/psalm": "^4.0@dev"
    },
    "autoload": {
        "psr-4": {
            "ForwardForce\\Daxko\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ForwardForce\\Daxko\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm -c psalm.xml --show-info=true",
        "test": "vendor/bin/phpunit --colors=always",
        "phpcs": "vendor/bin/phpcs src/"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Teodor Talov",
            "email": "teodor@forwardforce.io"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
